.cloudui-colorpicker{width:25px;height:27px;position:absolute;background:url(../img/cloudui-icon.png) no-repeat left -17px}#cloudui-colorpicker{position:absolute;z-index:999}#cloudui-colorpicker:focus{outline:none}#cloudui-colorpicker table{border:1px #000 solid;width:230px;border-collapse:collapse;border-spacing:0}#cloudui-colorpicker table td{height:10px;border-bottom:1px #000 solid;border-right:1px #000 solid}#cloudui-colorpicker .cloudui-colorpicker-title{background:#EFEFEF;height:30px;font-family:"MICORSOFT YAHEI";font-size:12px}#cloudui-colorpicker .cloudui-colorpicker-title div{margin-left:10px;width:60px;height:20px;line-height:20px;float:left;color:#000}#cloudui-colorpicker .cloudui-colorpicker-black{background:#000}#cloudui-colorpicker .cloudui-colorpicker-write{background:#FFF}