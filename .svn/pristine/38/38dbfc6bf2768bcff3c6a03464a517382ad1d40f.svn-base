#cloudui-popup{width:260px;height:180px;text-align:center;position:fixed;top:40%;left:50%;margin:-140px 0 0 -130px;background:#F8F8FA;display:none;z-index:1000;border-radius:5px;-moz-border-radius:5px;-webkit-border-radius:5px;box-shadow:5px 5px 5px #999;-moz-box-shadow:5px 5px 5px #999;-webkit-box-shadow:5px 5px 5px #999;opacity:0}#cloudui-popup p,#cloudui-popup div{height:30px;line-height:30px;color:#666}#cloudui-popup button{background:#FF6D60;border:1px solid #FF6D60;border-radius:4px;color:#fff;cursor:pointer;height:35px;line-height:33px;text-align:center;padding:0 20px}#cloudui-popup button em{font-style:normal}#cloudui-popup .btn-light{background:#FFF;border:1px solid #d5d5d5;border-radius:4px;color:#fff;cursor:pointer;height:35px;line-height:33px;color:#898e91;text-align:center;padding:0 20px}#cloudui-popup .btn-light:hover{background-color:#F8F8FA;transition:background-color .2s linear;-moz-transition:background-color .2s linear;-webkit-transition:background-color .2s linear;-o-transition:background-color .2s linear}#cloudui-popup .border-top{height:6px;overflow:hidden}#cloudui-popup .border-top div{height:12px;background:#C50205;border-radius:10px;-moz-border-radius:10px;-webkit-border-radius:10px}#cloudui-popup .title{font-size:18px;margin-top:10px;color:#333}#cloudui-popup .msg{height:30px;padding:0 30px;line-height:15px;margin:10px 0}#cloudui-popup .icon{width:32px;height:32px;margin:0 auto;margin-top:14px;background:url(../img/cloudui-icon.png) no-repeat}#cloudui-popup .icon-1{background-position:-96px -105px}#cloudui-popup .icon-2{background-position:-64px -105px}#cloudui-popup .icon-3{background-position:-32px -105px}#cloudui-popup .icon-4{background-position:left -105px}#cloudui-popup .confirm{display:none}#cloudui-popup .confirm .btn-light{margin-left:5px}