@charset "utf-8";
/*
 * 后台系统iframe窗体内容CSS样式
 */
body {color:#797979;background:#f1f2f7;font-family:PingFang SC,Lantinghei SC,'Microsoft YaHei';padding:0px !important;margin:0px !important;font-size:14px;text-rendering:optimizeLegibility;-webkit-font-smoothing:antialiased;-moz-font-smoothing:antialiased;}
ul li {list-style:none;}
input[type=checkbox],input[type=radio]{vertical-align: middle; margin:0;}
a,a:hover,a:focus {text-decoration:none;outline:none;}
::selection {background:#FF6C60;color:#fff;}
::-moz-selection {background:#FF6C60;color:#fff;}

/*面包屑*/
.position{height: 40px; line-height: 40px; padding: 0; margin: 0 22px;}
.position i {position: relative; top: 3px; width: 12px; height: 17px; margin: 0 5px 0 0; background: url(../img/icon-location.png) no-repeat;}
.position i.fa-home:before {content: "";}
.position .list{display: inline-block; font-size:14px; color:#d2d2d2;}
.position .list a{color:#667fa0; cursor: default;}
.position .list span{color:#797979;}
/*主体内容*/
/*.wrapper .row {margin-left: 0; margin-right: 0;}*/
.table-con{margin: 0 22px 0; padding:15px 24px; border-radius: 5px; background: #f8f8fa;}
.table-tab{margin:0 0 15px 0; border-bottom: 1px solid #ddd;}
.table-tab .tab-nav a{float:left; height: 36px; line-height: 36px; margin:0 6px 0 0; padding:0 25px; background: #edeff4; font-size:16px; color:#585858;}
.table-tab .tab-nav a.active{position: relative; bottom:-1px; border:1px solid #ddd; border-bottom: 1px solid #f8f8fa; background: #f8f8fa; font-weight: bold; color:#ff6c60;}
.table-tab .tab-btn{float:right;}
.btn {padding: 4px 12px;}
.table-padding{margin: 15px 0 0; overflow: auto;}
.table-padding td, .table-padding th {white-space: nowrap; word-break: keep-all;}
/*分页*/
.pages{padding:12px 0 0;}
.pages-left p{margin:0; line-height: 32px; color:#797979;}
.pages-left p span{color:#ff6c60;}
.pagination{margin:0;}
.pagination li a.active{background: #667fa0; color:#fff;}
/*底部*/
.site-footer {overflow: hidden; background: #5b6e84;color: #fff;}
.site-footer > div {margin: 10px 0;}
.go-top {float: right;width: 20px;height: 20px;margin-right: 1%;border-radius: 50%;-webkit-border-radius: 50%;background: rgba(255,255,255,.5);}
.go-top i {color: #2A3542;}

/*功能按钮*/
.table-btn .mr14{margin-right:14px;}
.table-btn .search label{float:left; margin:11px 40px 0 0; font-weight: normal; font-size: 12px;}
.table-btn .search label input{margin:0 5px 0 0; vertical-align: middle;}
.table-btn .search .search-con{float:left;}
.table-btn .search .search-con input{float:left; width:180px; line-height: 32px; margin:0 10px 0 0; padding:0 0 0 6px; border:1px solid #ddd; border-radius: 5px;}
.table-btn .pic-word{display: inline-block; border-radius: 5px; background: #f2f2f2;}
.table-btn .pic-word button{border:none; background: transparent;}
.table-btn .pic-word button.btn-ash{background: #949db0; color:#fff;}
.search-con label.fa-search {position: relative; margin: 0; height: 34px; padding: 0;}
.search-con label.fa-search:before { position:absolute; top: 10px; left: 14px; color: #a7adb1;}
.table-btn .search .search-con input {float: left; width: 217px; height: 30px; line-height: 28px; padding: 0 0 0 34px; margin: 0; border: 1px solid #ddd;  border-right: 0 none; border-radius: 5px 0 0 5px;}
.table-btn .search .search-con .btn-info { border-radius: 0 5px 5px 0;}

/*列表*/
.table-padding td, .table-padding th {white-space: nowrap; word-break: keep-all;}
.table-padding{margin: 15px 0 0; overflow: auto;}
.table.table-ash th{height:40px; line-height: 40px; padding:0 10px; border-bottom: 1px solid #ddd; background: #eef1f8; font-size:14px; font-weight: normal; color:#898e91;}
.table.table-ash th.w42{width:42px;}
.table.table-ash td{line-height: 24px; padding:10px; border-bottom: 1px solid #ddd; vertical-align:middle;}
.table.table-ash td a{color:#0C63AD;}
.table.table-ash td a:hover{color:#ff6c60;}
.table.table-ash td.touxiang img{border-radius: 100%;}
.table.table-ash td.recommend{vertical-align: top; text-align: right;}
.table.table-ash td.recommend .tj{display: inline-block; line-height: 18px; padding:0 6px; border-radius: 3px; background: #ff6c60; font-size:12px; color:#fff; white-space: nowrap;}
.table.table-ash td.red{color:#ff6c60;}
.table.table-ash td.blue{color:#3762a4;}
.table.table-ash td.green{color:#41cac0;}
.table.table-ash td.qgreen{color:#78cd51;}
.table.table-ash td .color-1{color:#667fa0;}
.table.table-ash td.center{text-align: center;}
.table.table-ash .pl-18{padding-left:18px;}
.table label{margin-bottom: 0;}
#list tr:hover, #list tr.active,#list tr.active td{background: #eef1f8;}
.table.table-ash tr >td:first-child{ border-left:3px solid #f8f8fa;}
.table.table-ash tr.active >td:first-child{border-left:3px solid #ff6c60;}
.table.table-ash tr:hover >td:first-child ,.table.table-ash tr.active:hover >td:first-child{border-left:3px solid #ff6c60;}

.item-selection{border:1px solid #eff2f7;}
.item-tree{width:273px; height: 500px; border-right: 1px solid #eff2f7;}
.item-title{height:39px; line-height:39px; background:#eff2f7; font-size:16px; color:#797979; text-align: center;}
/*树形菜单*/
.tree-nav{padding:20px 0 10px 0;}
.tree-nav > li >a{display: block; line-height: 40px;}
.tree-nav li a{margin:0 0 0 34px; font-size: 14px; color:#797979;}
.tree-nav li a:hover,.tree-nav li a.active{color:#ff6c60;}
.tree-nav li a i{margin:0 6px 0 0; font-size: 20px; vertical-align: middle;}
.tree-nav li >ul.tree-loader li a{display: block; line-height: 40px; margin:0; padding: 0 0 0 60px;}
.tree-nav li >ul.tree-loader li a.active{background: #ff6c60; color:#fff;}

.item-select{width:548px;}
.item-select .item-li{padding:18px 0 20px 30px; border-bottom: 1px solid #eff2f7;}
.item-select .item-li li{line-height: 28px; font-size:14px; color:#797979;}
.item-select .item-li li label{font-weight: normal;}
.item-select .item-li li label input{margin:0 8px 0 0;}

.item-select-li{padding:0 0 0 35px;}
.item-select-li h4{margin:20px 0 30px; font-size:16px; color:#797979;}
.item-select-li li{display: inline-block; margin:0 35px 14px 0; border:1px solid #a0a0a0;}
.item-select-li li p{float:left; display: inline-block; margin:0; padding:0 15px; line-height: 36px; font-size:14px; color:#797979;}
.item-select-li li span{display: inline-block; width:40px; line-height: 36px; background: #eff2f7; font-size:18px; color:#898e91; text-align: center; cursor: pointer;}

.withdrawn .info{padding:40px 0; font-size:16px; color:#797979; text-align: center;}
.withdrawn .texa{position: relative;}
.withdrawn .texa textarea{width:100%; height: 320px; padding:26px 0 0 20px; border:1px solid #e2e2e4;}
.withdrawn .texa p{position: absolute; top:26px; left:20px; font-size:14px; color:#c2c2c2;}
.withdrawn .texa p i{margin:0 8px 0 0;}

.name-info img{display:block; margin:10px auto;}
.name-info p{line-height: 35px; font-size:16px; color:#797979;}

/*弹窗表单*/
.mask{position: fixed; top:0; left:0; width:100%; height:100%; background: rgba(0,0,0,0.4); z-index: 999;}
.popup-content{padding:10px 20px; background: #fff; margin:10px 4px 10px 0;}
.popup-form{position: relative;}
.popup-title{height: 45px; margin:0 0 30px 0; padding:0 0 0 18px; background: #f7f7f7;}
.popup-title h3{display:inline-block; line-height: 45px; margin:0; font-size:18px; font-weight: normal; color:#666;}
.popup-title .times{float:right; line-height: 45px; font-size:14px; color:#797979;}
.navy-line{border-left:3px solid #78cd51;}
.yellow-line{border-left:3px solid #f1c500;}
.blue-line{border-left:3px solid #58c9f3;}
.red-line{border-left:3px solid #ff6c60;}
.wathet-line{border-left:3px solid #41cac0;}
.popup-form{margin:0 0 18px 0;}
.popup-form table{width:100%;}
.popup-form tr{display: block; margin:0 0 18px 0;}
.popup-form .field{width:110px; height:36px; line-height: 36px; font-size:14px; color:#797979; text-align: right; vertical-align: top;}
.popup-form .field span{position: relative; top:3px; margin:0 8px 0 0; color:#ff6c60;}
.popup-form .element{padding:0 0 0 18px;}
.popup-form .element .text{width:360px; height:36px; margin:0 10px 0 0; padding:0 0 0 8px;}
.popup-form .element .text.w564{width:564px;}
.popup-form .element .select{width:180px; height:36px;}
.popup-form .element label{display: inline-block; margin:0 20px 0 0; font-size:12px; font-weight: normal; color:#797979;}
.popup-form .element label input{display: inline-block; margin:0 8px 0 0;}
.popup-form .prompt{display: inline-block;}
.popup-form .prompt i{margin:0 10px 0 0; font-size:20px; color:#ff6c60; vertical-align: middle;}
.popup-form .element .element-texa textarea{line-height: 24px; margin:0 20px 0 0; padding:8px 0 0 8px; font-size:14px; color:#c2c2c2; vertical-align: middle;}
.popup-form .element .element-texa .w680{width:680px; height:134px;}
.popup-form .element .element-texa .w670{width:670px; height:134px;}
.popup-form .element .element-texa .w500{width:500px; height:134px;}
.popup-form .element .element-texa .w360{width:360px; height:134px;}
.popup-form .element .ele-btn{width:180px; line-height: 36px; border:none; border-radius: 5px; background: #505b6c; font-size:14px; color:#fff; text-align: center;}
.popup-form .popup-texa {display: block; width:100%;}
.popup-form .popup-texa .textarea{width:100%; height:130px; line-height: 20px; padding:6px 0 0 8px; overflow: hidden; resize: none;}
.popup-form .thumb-img{display: inline-block; margin:0 20px 0 20px; padding:4px; border:1px solid #ddd; border-radius: 5px; vertical-align: middle;}
.popup-form .thumb-img .pic{position: relative; overflow: hidden;}
.popup-form .thumb-img input{position: absolute; top:0; left:0; width:187px; height:141px; font-size:999em; overflow: hidden; z-index: 9; cursor: pointer; opacity: 0;}
.popup-form .thumb-img .th-btn{width:100%; height:29px; line-height: 29px; margin:6px 0 0; border:none; background: #495d74; color:#fff; text-align: center;}
.popup-form .thumb-img .th-btn:hover{background: #949caf;}
.popup-form .thumb-more h4{font-size:14px; color:#797979;}
.popup-form .thumb-more li{float:left; width:145px; margin:0 33px 0 0;}
.popup-form .thumb-more li .pic{overflow: hidden; position: relative; padding:3px; border:1px solid #ddd; border-radius: 5px;}
.popup-form .thumb-more li .pic input{position: absolute; top:0; left:0; font-size:999em; z-index: 9; cursor: pointer;}
.popup-form .thumb-more li .pic .cover{position: absolute; top:0; left:0; width:100%; height: 30px; line-height: 30px; background: rgba(0,0,0,0.3); z-index: 99}
.popup-form .thumb-more li .pic .cover h5{position: absolute; width:100%; font-size:12px; color:#fff; text-align: center;}
.popup-form .thumb-more li .pic .cover span{position: absolute; right:0; width: 20px; font-size:12px; color:#fff; text-align: center; cursor: pointer;}
.popup-form .thumb-more li .more-btn{padding:13px 0 0;}
.popup-form .thumb-more li .more-btn .btn1{float:left; line-height: 23px; padding:0 20px; border:none; border-radius: 4px; background: #ff6c60; font-size:12px; color:#fff;}
.popup-form .thumb-more li .more-btn .btn2{float:right; line-height: 23px; padding:0 20px; border:none; border-radius: 4px; background: #949caf; font-size:12px; color:#fff;}
.popup-form .thumb-more li .pic{border:1px solid #ddd; border-radius: 5px; padding:3px;}
.popup-form .thumb-more li .pic .cover-a a{display: block; width:137px; height: 103px; line-height: 103px; background: #f2f2f2; font-size:14px; color:#797979; text-align: center;}
.popup-form .upload{position: absolute; top:20px; right:20px;}
.popup-form .upload span{float:left; font-size:14px; color:#797979;}
.popup-form .upload .upload-img{position: relative; overflow: hidden; float:right; width:138px; height:138px; border:4px solid #f5f5f5; border-radius: 100%;}
.popup-form .upload .upload-img input{position: absolute; top:0; left:0; width:138px; height:138px; font-size:999em; cursor: pointer; opacity: 0;}
.popup-form .upload .upload-img p{position: absolute; bottom:0;left:0; width:138px; height:35px; line-height: 35px; margin:0; background: rgba(139,139,139,0.4); font-size:14px; color:#fff; text-align: center;}